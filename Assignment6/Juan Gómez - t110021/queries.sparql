PREFIX mv: <http://example.org/myVocabulary#>
PREFIX qb: <http://purl.org/linked-data/cube#>

SELECT ?dis WHERE {
  ?objects a qb:Observation ;
  mv:relatedDistrict ?dis ;
  mv:hasAccidentType mv:BicycleFall ;
  mv:numberOfAccidents ?y . }
